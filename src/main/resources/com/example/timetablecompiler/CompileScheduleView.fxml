<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="gridPane" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.timetablecompiler.controllers.CompileScheduleViewController" prefHeight="400.0" prefWidth="600.0">
    <columnConstraints>
        <ColumnConstraints />
        <ColumnConstraints />
        <ColumnConstraints />
        <ColumnConstraints />
        <ColumnConstraints />
        <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <FlowPane GridPane.rowIndex="0" GridPane.columnIndex="0" GridPane.columnSpan="6">
        <Button text="Добавить правила"/>
        <Label text="Добавлено правил: 0"/>
        <Button onAction="#clickToGenerateSchedule" GridPane.columnIndex="0" GridPane.rowIndex="0" text="Сгенерировать"/>
        <Label text="Загрузить для:"/>
        <Button onAction="#clickToLoadForA" text="10А"/>
        <Button onAction="#clickToLoadForB" text="10Б"/>
        <Label text="Удалить для:"/>
        <Button onAction="#clickToDeleteForA" text="10А"/>
        <Button onAction="#clickToDeleteForB" text="10Б"/>
    </FlowPane>
</GridPane>
